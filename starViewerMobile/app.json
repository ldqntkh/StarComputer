{
  "expo": {
    "name": "StarMiningViewer",
    "description": "StarMining viewer, ETH crypto viewer",
    "slug": "starMiningViewer",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/images/logo.png",
    "splash": {
      "image": "./public/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1e1e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
