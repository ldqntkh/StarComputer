{
  "expo": {
    "name": "StarMiningViewer",
    "description": "StarMining viewer, ETH crypto viewer",
    "slug": "starMiningViewer",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./public/images/icon.png",
    "splash": {
      "image": "./public/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1e1e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist":{  
        "NSCameraUsageDescription":"StarMining muốn sử dụng camera để quét mã Qrcode. Bạn có muốn cho StarMining sử dụng camera?"
     },
      "supportsTablet": true,
      "bundleIdentifier" : "com.starcomputer.starminingviewer"
    }
  }
}
